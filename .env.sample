# Elasticsearch Cloud Configuration
# Copy this file to .env and fill in your actual credentials

# Elasticsearch Endpoint (Cloud or Local)
# Example: https://my-deployment.es.us-central1.gcp.elastic.cloud:443
ES_ENDPOINT=https://your-deployment.es.region.gcp.elastic.cloud:443

# Elasticsearch API Key
# Get this from Kibana -> Stack Management -> API Keys
ES_API_KEY=your_api_key_here

# Elasticsearch Index Name
# The index where incident tickets will be stored
ES_INDEX=incident_tickets_uploaded
